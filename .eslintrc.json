{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["react-app", "airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2023,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "off",
    "prefer-const": "off",
    "arrow-body-style": "off",
    "import/no-unresolved" : "off"
  }
}
